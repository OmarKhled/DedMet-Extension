{
  "name": "got",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["cnt.js"]
    }
  ],
  "permissions": ["webRequest", "tabs", "<all_urls>"],
  "background": {
    "matches": ["<all_urls>"],
    "scripts": ["bg.js"],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
