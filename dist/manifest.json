{
  "name": "got",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://courses.nu.edu.eg/*"],
      "js": ["cnt.js"]
    }
  ],
  "permissions": ["webRequest", "tabs", "*://courses.nu.edu.eg/*"],
  "background": {
    "matches": ["*://courses.nu.edu.eg/*"],
    "scripts": ["bg.js"],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
